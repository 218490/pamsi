#ifndef IGRAF_HH
#define IGRAF_HH

#include <iostream>
using namespace std;

/*!
 *\file
 *\brief
 *Interface Grafu
 */

 class IGraf{

 public:
  virtual void DodajW(int pozycja)=0;
  virtual bool DodajK(int poczatek,int koniec, int waga=1)=0;
  virtual bool UsunK(int poczatek,int koniec)=0;
  virtual bool UsunW(int pozycja)=0;
 };

#endif
